{
  "name": "shuttle-store-sample",
  "version": "1.0.0",
  "description": "Angular2 + Store Integration",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:server:w\" \"npm run build:client:w\" \"npm run express\" ",
    "build:server": "gulp compile",
    "build:server:w": "gulp watch",
    "build:client": "webpack",
    "build:client:w": "webpack --watch",
    "prebuild": "",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\" ",
    "build:w": "concurrently \"webpack --watch\" \"gulp watch\" ",
    "tslint": "tslint -c tslint.json src*/**/*.ts",
    "express": "concurrently \"node ./src-server/express.js\" \"gulp browsersync\" ",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovrmrw/shuttle-store-sample.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ovrmrw/shuttle-store-sample/issues"
  },
  "homepage": "https://github.com/ovrmrw/shuttle-store-sample#readme",
  "devDependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.12.8",
    "codelyzer": "0.0.19",
    "concurrently": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-ignore": "^2.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^2.13.4",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router-deprecated": "^2.0.0-rc.1",
    "babel-polyfill": "^6.9.0",
    "body-parser": "^1.15.1",
    "c3": "^0.4.11",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "jquery": "^2.2.3",
    "lodash": "^4.12.0",
    "path": "^0.12.7",
    "rxjs": "^5.0.0-beta.7",
    "zone.js": "^0.6.12"
  }
}
