{
  "name": "shuttle-store-sample",
  "version": "1.0.0",
  "description": "Angular2 + Store Integration",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "prestart": "npm run build",
    "start": "concurrently -k \"npm run build:w\" \"npm run express\" ",
    "build:server": "gulp compile",
    "build:server:w": "gulp watch",
    "build:client": "webpack",
    "build:client:w": "webpack --watch",
    "build": "npm run build:client && npm run build:server",
    "build:w": "concurrently -k \"npm run build:client:w\" \"npm run build:server\" ",
    "tslint": "tslint -c tslint.json src*/**/*.ts",
    "express": "concurrently -k \"node ./src-server/express.js\" \"gulp browsersync\" ",
    "express:e2e": "concurrently -k \"node ./src-server/express.js\" \"gulp browsersync:e2e\" ",
    "build:test": "webpack --config test/webpack.config.test.js",
    "build:test:w": "webpack --config test/webpack.config.test.js --watch",
    "karma": "npm run build:test && karma start --single-run",
    "karma:w": "npm run build:test && concurrently -k \"karma start\" \"npm run build:test:w\" ",
    "nightwatch": "npm run build:server && npm run build:test && concurrently -k \"npm run express:e2e\" \"gulp nightwatch\" ",
    "nightwatch:w": "npm run build:server && npm run build:test && concurrently -k \"npm run build:test:w\" \"npm run express:e2e\" \"gulp nightwatch:w\" ",
    "test": "npm run karma && npm run nightwatch",
    "test:w": "concurrently -k \"npm run karma:w\" \"npm run nightwatch:w\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovrmrw/shuttle-store-sample.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ovrmrw/shuttle-store-sample/issues"
  },
  "homepage": "https://github.com/ovrmrw/shuttle-store-sample#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "babel-plugin-espower": "^2.2.0",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.12.8",
    "chromedriver": "^2.21.2",
    "codelyzer": "0.0.19",
    "concurrently": "^2.1.0",
    "core-js": "^2.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-ignore": "^2.0.1",
    "gulp-nightwatch": "^0.2.9",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^2.13.4",
    "html-loader": "^0.4.3",
    "jasmine": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.0",
    "nightwatch": "^0.9.4",
    "phantomjs-prebuilt": "^2.1.7",
    "power-assert": "^1.4.1",
    "selenium-server-standalone-jar": "^2.53.0",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10",
    "typings": "^1.0.4",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/core": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/router-deprecated": "^2.0.0-rc.2",
    "@ngrx/core": "^1.0.1",
    "@ngrx/router": "^1.0.0-beta.1",
    "babel-polyfill": "^6.9.0",
    "body-parser": "^1.15.1",
    "c3": "^0.4.11",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "jquery": "^2.2.3",
    "level-js": "^2.2.4",
    "leveldown": "^1.4.6",
    "levelup": "^1.3.2",
    "lodash": "^4.12.0",
    "moment": "^2.13.0",
    "path": "^0.12.7",
    "rxjs": "^5.0.0-beta.7",
    "toastr": "^2.1.2",
    "zone.js": "^0.6.12"
  }
}